{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "https://modrinth.com/mod/hitmarker/",
        "sources": "https://github.com/vadik317/hitmarkermod/"
    },
    "license": "${license}",
    "icon": "assets/modid/${mod_id}.png",
    "environment": "*",
    "entrypoints": {
        "main": [
          "vadik.hitmarker.HitMarkerFabric"
        ],
        "client": [
            "vadik.hitmarker.client.HitMarkerClientFabric"
        ]
    },
    "mixins": [
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.14",
        "fabric": "*",
        "minecraft": ">=1.21",
        "java": ">=17"
    },
    "suggests": {
        "another-mod": "*"
    }
}
  